        <form action="/fileupload/form" method="POST">
         <br>
         <div class="h2">
            <h2><u><b>List of Form Uploads</b><br> </u></h2>
         </div>

    <div class="border">
            <table class="table table-hover">
        

                <tr>
                    <th>First Name</th>
                    <th>Last name</th>
                    <th>Date Submitted</th>
                    <th>File Name</th>
                    <th>Title</th>
                    <th>Action</th>
                </tr>
            <% etdList.forEach(function(value) { %>
            <tr>
                <td>
                    <%= value.Creators_first_name %>
                </td>
                <td>
                    <%= value.Creators_last_name %>
                </td>
                <td>
                    <%= value.Date_of_Issue %>
                </td>
    
                
                <td>
                    <%= value.file %>
                </td>
                
                <td>
                    <%= value.Title %>
                </td>

                <td>
        
                    <button class="btn btn-primary" onclick="location.href='/fileupload/viewsubmissions/view/<%= value._id %>'" type="button">View</button>
                    <button class="btn btn-info" onclick="location.href='/fileupload/viewsubmissions/editprofile/<%= value._id %>'" type="button">Edit</button>
                    <button class="btn btn-success" onclick="location.href='/fileupload/viewsubmissions/sync/<%= value._id %>'" type="button">Syncronise</button>
                    <button class="btn btn-danger" onclick="location.href='/fileupload/viewsubmissions/delete/<%= value._id %>'" type="button">Delete</button>

            
                    <!-- <button type="button" class="btn btn-danger">Danger</button> -->
                </td>   
                   
                
            
           
        </tr>    
                       
                    <% }) %> 
            
       
            
            
         </div>
          
      </form>
    
    </table>
</div>
     </div>
    </div>
  </div>




