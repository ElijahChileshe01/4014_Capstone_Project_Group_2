
<!DOCTYPE html>
<html lang="en">
<head>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">

  <style type="text/css">


        
        body{
            
         
         padding-left: 35px;
         padding-right: 35px;
         padding-top: 20px;
         background-image: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)), url(/images/newpic4.jpg);
         background-size: cover;
         
        }
     

     .h2{
        text-align: center;
      color: aliceblue;
   }

    .border{

        background-color: gainsboro;
        padding: 20px;
        border: 3px solid black!important;
        border-radius: 20px;
       
    } 


  </style >
</head>
<body>
    <div class="body">

        <form action="/fileupload/form" method="POST">
         <br>
         <div class="h2">
            <h1><u><b>List of Form Uploads</b><br> </u></h1>
         </div>

    <div class="border">
            <table class="table table-hover">
        

                <tr>
                    <th>First Name</th>
                    <th>Last name</th>
                    <th>Title</th>
                    <th>Date Submitted</th>
                    <!-- <th>File Name</th> -->
                    <th>Action</th>
                </tr>
            <% etdList.forEach(function(value) { %>
            <tr>
                <td>
                    <%= value.Creators_first_name %>
                </td>
                <td>
                    <%= value.Creators_last_name %>
                </td>
                
                
                <td>
                    <%= value.Title %>
                </td>


                <td>
                    <%= value.Date_of_Issue %>
                </td>
    
<!--                 
                <td>
                    <%= value.file %>
                </td> -->
                
                <td>
        
                    <button class="btn btn-primary" onclick="location.href='/fileupload/viewsubmissions/view/<%= value._id %>'" type="button">View</button>
                    <button class="btn btn-warning" onclick="location.href='/fileupload/viewsubmissions/editprofile/<%= value._id %>'" type="button">Edit</button>
                    <button class="btn btn-success" onclick="location.href='/fileupload/viewsubmissions/sync/<%= value._id %>'" type="button">Syncronise</button>
                    <button class="btn btn-danger" onclick="location.href='/fileupload/viewsubmissions/delete/<%= value._id %>'" type="button">Delete</button>

            
                    <!-- <button type="button" class="btn btn-danger">Danger</button> -->
                </td>   
                   
                
            
           
        </tr>    
                       
                    <% }) %> 
            
       
            
            
         </div>
          
      </form>
    
    </table>
</div>
     </div>
    </div>
  </div>

</div>
</body>

</html>











